<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.terminator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Terminator">
        <activity android:name=".DisplayActivity"></activity>
        <activity android:name=".FormActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".BottomNavigation" />
        <activity android:name=".CommercialActivity" />
        <activity android:name=".PestPreventionActivity" />
        <activity android:name=".OutdoorActivity" />
        <activity android:name=".FumigationActivity" />
        <activity android:name=".NavigationDrawerActivity" />
        <activity android:name=".Homepage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>